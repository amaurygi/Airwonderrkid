<h1> All Properties </h1>

<% @properties.each do |property| %>
<p>  <%= link_to property.name, property_path(property), class: 'btn btn-flat' %> ||
  <%= link_to "edit #{property.name}", edit_property_path(property), class: 'btn btn-flat' %> ||
  <%= link_to "delete #{property.name}", property_path(property), method: :delete, class: 'btn btn-flat' %>
</p>
<% end %>
