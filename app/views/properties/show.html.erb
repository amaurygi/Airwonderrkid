<h1> Property </h1>

<p><%= @property.name %> <%= @property.address %> <%= @property.price  %></p>

<p><%= link_to 'Book', new_property_booking_path(@property), class: 'btn btn-primary'%></p>
<p><%= link_to 'Go back', properties_path, class: 'btn btn-primary' %></p>
<p><%= link_to 'Edit', edit_property_path(@property), class: 'btn btn-primary' %></p>
<p> <%= link_to 'Delete', property_path(@property), method: :delete, class: 'btn btn-primary', data: { confirm: 'Are you sure you want to delete this list?' } %> </p>

